<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="247.0" prefWidth="397.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReplyEventController">
   <children>
      <Label layoutX="110.0" layoutY="29.0" text="JOIN EVENT " textFill="#2faf6f">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Label fx:id="eventIDLabel" layoutX="218.0" layoutY="30.0" text="EVENTID" textFill="#2faf6f">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="68.0" layoutY="87.0" prefHeight="27.0" prefWidth="333.0" text="Are you sure you want to Join the Event?">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="94.0" layoutY="156.0" mnemonicParsing="false" onAction="#YesClicked" prefHeight="25.0" prefWidth="76.0" text="YES" textFill="#2faf6f">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Button layoutX="239.0" layoutY="156.0" mnemonicParsing="false" onAction="#NoClicked" prefHeight="25.0" prefWidth="76.0" text="NO" textFill="#2faf6f">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Button>
      <Label fx:id="usernameLabel" layoutX="14.0" layoutY="16.0" text="Username" />
   </children>
</AnchorPane>
